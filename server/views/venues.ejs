<html lang="en">
  <head>
    <meta charset="UTF-8">
    <base href="/">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0,user-scalable=no">

    <!-- CSS Libraries -->
 	<link rel="stylesheet" href="css/bootstrap.css">
    <!-- CSS stylesheets -->
    <link rel="stylesheet" href="css/stylesheet.css">

    <title>Eat36Five</title>
  </head>

  <body>
	
    <!--_____________________-->
    <!--    Main Container   -->
    <!--_____________________-->
    <div class="OuterMainContainer  ">
    	

		<nav class="navbar navbar-default" style="margin-bottom: 0px">
			<div class="container-fluid">
				<div class="navbar-header">
					
					<a class="navbar-brand" href="/">Eat36Five</a>
					</div><!-- end of navbar header -->
					</div><!-- end of container-fluid -->
					</nav><!-- end of navigation bar -->
					<div id="spinner">
						<span id="first" class="ball"></span>
						<span id="second" class="ball"></span>
						<span id="third" class="ball"></span>
					</div>
					<div class="containers">
						<div class="row">
							<div class="col-xs-12 col-sm-6 col-md-6 searchVenueContainer">
								<div class="SearchVenueFilterContainer">
									<h4>
									<p><small>Showing <b><%= data.length %></b> results</small></p>
									<button
										class="btn btn-primary btn-md"
										type="button"
										data-toggle="collapse"
										data-target="#collapseExample"
										aria-controls="collapseExample"
										aria-expanded="false"
										>
										Show Filters
									</button>
									</h4>
									
									<div class="collapse" id="collapseExample">
										<div class="well clearfix">
											
											<div class="dropDownSearchList">
												
												<h5><strong>SortBy</strong></h5>
												<ul class="innerDropDownSearchList">
													
													<li>
														<a href=""  >Best Match</a>
													</li>
													<li >
														<a href=""  >Highest Rated</a>
													</li>
													<li>
														<a href="" >Lowest Rated</a>
													</li>
													</ul><!-- end of innerDropDownSearchList -->
													
													</div><!-- end of dropDownSearchList -->
													<div class="dropDownSearchList">
														
														<h5><strong>Price</strong></h5>
														<ul class="innerDropDownSearchList">
															
															<li>
																<a href="" >Most Expensive</a>
															</li>
															<li>
																<a href="" >Least Expensive</a>
															</li>
														</ul>
												</div><!-- end of dropDownSearchList -->
											</div><!-- end of well -->
										</div><!-- end of collapse -->									
								</div> <!-- SearchVenueFilterContainer -->
							<div id="react-container">
								 <%- helloComponentMarkup %> 
							</div>
							</div><!-- end of col-md-6 -->
							<div class="col-xs-12 col-sm-6 col-md-6" style="padding: 0">
				
								<div id="mapCanvas" style="width: 100%; height: 92%"></div>					
							</div><!-- end of col-md-6 -->											
				</div><!-- end of row -->
		</div><!-- end of container -->

    </div><!-- end of container -->



    <!-- Libs -->

  </body>
</html>


							